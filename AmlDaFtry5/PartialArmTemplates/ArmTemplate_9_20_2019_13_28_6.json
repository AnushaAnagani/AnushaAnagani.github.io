{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "AmlDaFtry5"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/predictions')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureStorageLinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"name": {
							"type": "string"
						},
						"properties": {
							"type": "object",
							"properties": {
								"type": {
									"type": "string"
								},
								"linkedServiceName": {
									"type": "string"
								},
								"structure": {
									"type": "array"
								},
								"typeProperties": {
									"type": "object",
									"properties": {
										"folderPath": {
											"type": "string"
										},
										"fileName": {
											"type": "string"
										},
										"partitionedBy": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"name": {
														"type": "string"
													},
													"value": {
														"type": "object",
														"properties": {
															"type": {
																"type": "string"
															},
															"date": {
																"type": "string"
															},
															"format": {
																"type": "string"
															}
														}
													}
												}
											}
										},
										"format": {
											"type": "object",
											"properties": {
												"type": {
													"type": "string"
												},
												"firstRowAsHeader": {
													"type": "boolean"
												}
											}
										}
									}
								},
								"external": {
									"type": "boolean"
								},
								"availability": {
									"type": "object",
									"properties": {
										"frequency": {
											"type": "string"
										},
										"interval": {
											"type": "integer"
										}
									}
								}
							}
						}
					}
				}
			},
			"dependsOn": []
		}
	]
}