{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "AmlDaFtry5"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureML1_apiKey": {
			"type": "secureString",
			"metadata": "Secure string for 'apiKey' of 'AzureML1'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureML1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureML",
				"typeProperties": {
					"mlEndpoint": "https://ussouthcentral.services.azureml.net/subscriptions/0bf180ae7b974c4b9c741296b25e462a/services/5471404e7c94451eae7a946cfb8fbd60/jobs?api-version=2.0",
					"apiKey": {
						"type": "SecureString",
						"value": "[parameters('AzureML1_apiKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureStorageLinkedService')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureStorageLinkedService_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/integrationRuntime1')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/patientData')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureStorageLinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"folderPath": "patient_data/2017",
						"container": "bigdata"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"name": {
							"type": "string"
						},
						"properties": {
							"type": "object",
							"properties": {
								"type": {
									"type": "string"
								},
								"linkedServiceName": {
									"type": "string"
								},
								"structure": {
									"type": "array"
								},
								"typeProperties": {
									"type": "object",
									"properties": {
										"folderPath": {
											"type": "string"
										},
										"fileName": {
											"type": "string"
										},
										"partitionedBy": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"name": {
														"type": "string"
													},
													"value": {
														"type": "object",
														"properties": {
															"type": {
																"type": "string"
															},
															"date": {
																"type": "string"
															},
															"format": {
																"type": "string"
															}
														}
													}
												}
											}
										},
										"format": {
											"type": "object",
											"properties": {
												"type": {
													"type": "string"
												},
												"firstRowAsHeader": {
													"type": "boolean"
												}
											}
										}
									}
								},
								"external": {
									"type": "boolean"
								},
								"availability": {
									"type": "object",
									"properties": {
										"frequency": {
											"type": "string"
										},
										"interval": {
											"type": "integer"
										}
									}
								},
								"policy": {
									"type": "object",
									"properties": {
										"externalData": {
											"type": "object",
											"properties": {
												"retryInterval": {
													"type": "string"
												},
												"retryTimeout": {
													"type": "string"
												},
												"maximumRetry": {
													"type": "integer"
												}
											}
										}
									}
								}
							}
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureStorageLinkedService')]"
			]
		}
	]
}